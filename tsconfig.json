{
  "compilerOptions": {
    "target": "es2015",
    "module": "system",
    "moduleResolution": "node",
    "lib": ["ES6", "ES5", "ES2015.Iterable", "DOM", "DOM.Iterable"],
    "declaration": true,
    "noEmit": false,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "outFile": "./dist/index.js"
  },
  "include": ["./lib/**/*.ts"]
}
